citHeader("To cite manydata in publications use:")

bibentry(
     bibtype = "Manual",
     title = "manydata: The portal for global governance data",
     author = c(
       person("James", "Hollway", 
       role = c("cre", "aut", "ctb"),
       email = "james.hollway@graduateinstitute.ch")
     ),
     year = "2021",
     url = "https://github.com/globalgov/manydata"
)
