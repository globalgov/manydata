citHeader("To cite manydata in publications use:")

citEntry(
  entry    = "Manual",
  title    = "manydata: An R Portal for Ensembled Global Governance Data",
  author   = "James Hollway",
  year     = "2021",
  version  = "0.5.0",
  url      = "https://github.com/globalgov/manydata",
  textVersion = paste("J. Hollway. manydata: An R Portal for Ensembled Global Governance Data. 2021."
  )
)
